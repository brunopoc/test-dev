CREATE TABLE `crud_base`.`tabCarro` (
  `intId` INT NOT NULL AUTO_INCREMENT,
  `intAno` INT NULL,
  `strModelo` VARCHAR(45) NOT NULL,
  `strMarca` VARCHAR(45) NOT NULL,
  PRIMARY KEY (`intId`));

